{
  "name": "neuro-snap",
  "version": "1.0.0",
  "description": "Personality test analysis backend with AI integration",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "logs": "node scripts/view-logs.js",
    "logs:stats": "node scripts/log-stats.js",
    "logs:api": "node scripts/view-logs.js --api --details",
    "logs:db": "node scripts/view-logs.js --db --details",
    "logs:errors": "node scripts/view-logs.js --error",
    "logs:clear": "rm -f logs/*.log && echo 'Logs cleared successfully'",
    "knowledge:init": "node scripts/init-knowledge-base.js",
    "knowledge:stats": "curl -H 'X-Frontend-Key: your-unique-frontend-app-key-2024' http://localhost:8080/api/analysis/knowledge/stats | jq",
    "analysis:preview": "curl -X POST -H 'Content-Type: application/json' -H 'X-Frontend-Key: your-unique-frontend-app-key-2024' http://localhost:8080/api/analysis/preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "@types/compression": "^1.8.1",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.9.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.26.0",
    "socks-proxy-agent": "^8.0.5",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
